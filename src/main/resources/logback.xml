<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} - [%X{akkaSource}] %logger{5} %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="co.assign" level="debug"/>

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>

</configuration>